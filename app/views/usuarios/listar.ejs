<!DOCTYPE html>
<html lang="pt-BR">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Listagem de Usuários</title>
</head>
<body>
    <h1>Listagem de Usuários</h1>
    <ul>
        <% usuarios.forEach(usuario => { %>
            <li><%= usuario.nome %> - <%= usuario.email %></li>
        <% }); %>
    </ul>
</body>
</html>
