
<%- include ('../../partials/header.ejs'); %>
<%- include ('../../partials/navbarhome.ejs'); %>

<div class="container">
    <hr>
    <h2>Usuários</h2>

    <a class="btn btn-success" href="/admin/users/create">Criar novo usuário</a>
    <div class="card mt-5">


        <table class="table table-hover">

            <tr>
                <th>id</th>
                <th>Senha</th>
            </tr>

            <% user.forEach((user)=>{ %>

               <tr>
                    <td><%= user.id %></td>
                    <td><%= user.email %></td>
                </tr>

            <% }) %>

            </table>

    </div>

    
</div>


